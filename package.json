{
  "name": "lunacat-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "yarn build && pm2-runtime start ecosystem.config.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "prebuild": "yarn",
    "build": "tsc",
    "docker": "yarn install && cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint": "eslint --fix \"src/**/*.ts\" \"__tests__/**/*.test.ts\"",
    "lint:init": "eslint --init",
    "test": "NODE_ENV=test && jest --runInBand --detectOpenHandles",
    "test:cov": "cross-env NODE_ENV=test && jest --coverage --forceExit",
    "test:docker": "docker exec -it server yarn test --forceExit",
    "test:watch": "docker exec -it server yarn test --watch",
    "test:docker-cov": "docker exec -it server yarn test:cov"
  },
  "dependencies": {
    "@slack/webhook": "^5.0.3",
    "aws-sdk": "^2.696.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "connect-redis": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.8.36",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "envalid": "^7.0.0",
    "express": "~4.16.1",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "http-errors": "~1.6.3",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.9",
    "mongoose": "5.11.15",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.10",
    "nodemon": "^2.0.4",
    "pm2": "^4.5.6",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "slack-node": "^0.1.8",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^4.1.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.12.17",
    "@jest/globals": "^26.4.2",
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.28",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.4",
    "form-data": "^3.0.0",
    "jest": "^26.4.2",
    "mongodb-memory-server": "^6.9.3",
    "prettier": "^2.2.1",
    "random-string": "^0.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1"
  }
}
